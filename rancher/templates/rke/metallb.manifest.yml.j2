---
apiVersion: v1
kind: Namespace
metadata:
  name: {{ rke_metallb_namespace }}

---
apiVersion: v1
kind: ConfigMap
metadata:
  namespace: {{ rke_metallb_namespace }}
  name: config
data:
  config: |
    address-pools:
    - name: metallb
      protocol: layer2
      addresses:
        - {{ rke_metallb_ips }}
